function docker-stopall {
    if [[ $(docker ps -q | wc -l) -gt 0 ]]; then
        docker stop $(docker ps -q) > /dev/null;
    fi    
}

function docker-start-postgres {
    docker run --rm \
    --name postgres \
    -p 5000:5432 \
    -e POSTGRES_DB=mgmt \
    -v ~/state/postgres:/var/lib/postgresql/data \
    -d postgres:9.6.4
}
